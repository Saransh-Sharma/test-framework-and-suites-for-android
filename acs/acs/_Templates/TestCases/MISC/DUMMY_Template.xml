<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>DUMMY</UseCase>
    <Description>Dummy Usecase used for testing ACS framework</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcMaxAttempt>1</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <TcExpectedResult>PASS</TcExpectedResult>
    <IsCritical>False</IsCritical>
    <Parameters>
        <Parameter>
            <!-- Test step where failures will be generated
                - SETUP
                - RUNTEST
                - TEARDOWN
                - RANDOM (between above choices)
                Can be a list of value (separate by ';'), in that case corresponding
                b2b or retry iteration must be set -->
            <Name>STEP</Name>
            <Value>RUNTEST</Value>
        </Parameter>
        <!-- Possible return code
            - PASS (or SUCCESS)
            - FAIL (or FAILURE)
            - BLOCKED
            - VALID
            - INVALID
            - INCONCLUSIVE
            - RANDOM (between above choices)
            Can be a list of value (separate by ';'), in that case corresponding
            b2b or retry iteration must be set -->
        <Parameter>
            <Name>RETURN_CODE</Name>
            <Value>RANDOM</Value>
        </Parameter>
        <!-- Comment associated with verdict -->
        <Parameter>
            <Name>COMMENT</Name>
            <Value>Dummy failure occured</Value>
        </Parameter>
        <Parameter>
            <Name>DURATION</Name>
            <Value>0</Value>
        </Parameter>
        <!-- Whether the UC is semi auto or not (0 = False, 1 = True) -->
        <Parameter>
            <Name>SEMI_AUTO</Name>
            <Value>0</Value>
        </Parameter>
    </Parameters>
</TestCase>