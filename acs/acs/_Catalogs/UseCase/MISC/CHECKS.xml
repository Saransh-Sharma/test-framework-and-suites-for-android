<?xml version="1.0" encoding="iso-8859-1"?>
<UseCases xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../usecase.xsd">
    <UseCase Id="DUMMY" Domain="MISC" SubDomain="CHECKS" OS="N/A" Status="DONE">
        <ClassName>acs.UseCase.Misc.DUMMY.Dummy</ClassName>
        <Description>Dummy UC for testing ACS framework</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="STEP" type="STRING" isOptional="false">
                <Description>Test step where failures will be generated
                    - SETUP
                    - RUNTEST
                    - TEARDOWN
                    - RANDOM (between above choices)
                    Can be a list of value (separate by ';'), in that case corresponding
                    b2b or retry iteration must be set
                </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="RETURN_CODE" type="STRING" isOptional="false">
                <Description>Possible return code
                    - SUCCESS
                    - FAILURE
                    - RANDOM (between above choices)
                    Can be a list of value (separate by ';'), in that case corresponding
                    b2b or retry iteration must be set
                </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="COMMENT" type="STRNG" isOptional="false">
                <Description>Comment associated with verdict</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DURATION" type="INTEGER" isOptional="false">
                <Description>Duration for each steps in seconds</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
</UseCases>
