<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>DUMMY</UseCase>
  <Description>Run ACS campaign with DUMMY UC set for PASS, FAIL, FAIL verdict:
               TcMaxAttempt = 3
               TcAcceptanceCriteria = 2</Description>
  <TcExpectedResult>FAIL</TcExpectedResult>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>False</b2bContinuousMode>
  <TcMaxAttempt>3</TcMaxAttempt>
  <TcAcceptanceCriteria>2</TcAcceptanceCriteria>
  <Parameters>
    <Parameter>
      <Name>STEP</Name>
      <Value>RUNTEST;RUNTEST;RUNTEST</Value>
    </Parameter>
    <Parameter>
      <Name>RETURN_CODE</Name>
      <Value>SUCCESS;FAILURE;FAILURE</Value>
    </Parameter>
    <Parameter>
      <Name>COMMENT</Name>
      <Value>Two Dummy failures occured</Value>
    </Parameter>
    <Parameter>
      <Name>DURATION</Name>
      <Value>0</Value>
    </Parameter>
  </Parameters>
</TestCase>
