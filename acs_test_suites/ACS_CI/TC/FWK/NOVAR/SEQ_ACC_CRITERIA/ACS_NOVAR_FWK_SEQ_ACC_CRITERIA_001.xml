<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>DUMMY</UseCase>
  <Description>Run ACS campaign with DUMMY UC set for PASS verdict after 3 tries
               TcMaxAttempt = 3
               TcAcceptanceCriteria = 3</Description>
  <TcExpectedResult>PASS</TcExpectedResult>
  <TcMaxAttempt>3</TcMaxAttempt>
  <TcAcceptanceCriteria>3</TcAcceptanceCriteria>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>False</b2bContinuousMode>
  <Parameters>
    <Parameter>
      <Name>STEP</Name>
      <Value>RUNTEST</Value>
    </Parameter>
    <Parameter>
      <Name>RETURN_CODE</Name>
      <Value>SUCCESS</Value>
    </Parameter>
    <Parameter>
      <Name>COMMENT</Name>
      <Value>Dummy success occured</Value>
    </Parameter>
    <Parameter>
      <Name>DURATION</Name>
      <Value>0</Value>
    </Parameter>
  </Parameters>
</TestCase>
