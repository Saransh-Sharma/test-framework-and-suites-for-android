<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>DUMMY</UseCase>
  <Description>Run ACS campaign with DUMMY UC set for PASS verdict: Verdict PASS expected after 1 try
               ACS will set to default value 1 if TcMaxAttempt is = 0
               TcMaxAttempt = 0
               TcAcceptanceCriteria = 1</Description>
  <TcExpectedResult>PASS</TcExpectedResult>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>False</b2bContinuousMode>
  <TcMaxAttempt>0</TcMaxAttempt>
  <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
  <Parameters>
    <Parameter>
      <Name>STEP</Name>
      <Value>RUNTEST</Value>
    </Parameter>
    <Parameter>
      <Name>RETURN_CODE</Name>
      <Value>SUCCESS</Value>
    </Parameter>
    <Parameter>
      <Name>COMMENT</Name>
      <Value>Dummy success occured</Value>
    </Parameter>
    <Parameter>
      <Name>DURATION</Name>
      <Value>0</Value>
    </Parameter>
  </Parameters>
</TestCase>
