<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>DUMMY</UseCase>
  <Description>Run ACS campaign with DUMMY UC set for FAIL, FAIL, PASS verdict:Verdict PASS expected after 3 tries
               TcMaxAttempt = 3
               TcAcceptanceCriteria = 1</Description>
  <TcExpectedResult>PASS</TcExpectedResult>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>False</b2bContinuousMode>
  <TcMaxAttempt>3</TcMaxAttempt>
  <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
  <Parameters>
    <Parameter>
      <Name>STEP</Name>
      <Value>RUNTEST;RUNTEST;RUNTEST</Value>
    </Parameter>
    <Parameter>
      <Name>RETURN_CODE</Name>
      <Value>FAILURE;FAILURE;SUCCESS</Value>
    </Parameter>
    <Parameter>
      <Name>COMMENT</Name>
      <Value>Two Dummy failures occured</Value>
    </Parameter>
    <Parameter>
      <Name>DURATION</Name>
      <Value>0</Value>
    </Parameter>
  </Parameters>
</TestCase>
