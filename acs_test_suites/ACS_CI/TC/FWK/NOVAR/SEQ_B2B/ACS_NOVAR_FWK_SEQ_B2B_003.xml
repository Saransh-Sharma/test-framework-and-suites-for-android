<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>DUMMY</UseCase>
  <Description>"Run ACS campaign with DUMMY UC set for 4 PASS and 1 FAIL verdict:
                b2bIteration = 5
                b2bContinuousMode = False"</Description>
  <TcExpectedResult>PASS</TcExpectedResult>
  <b2bIteration>5</b2bIteration>
  <b2bContinuousMode>False</b2bContinuousMode>
  <TcMaxAttempt>1</TcMaxAttempt>
  <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
  <Parameters>
    <Parameter>
      <Name>STEP</Name>
      <Value>RUNTEST;RUNTEST;RUNTEST;RUNTEST;RUNTEST</Value>
    </Parameter>
    <Parameter>
      <Name>RETURN_CODE</Name>
      <Value>SUCCESS;SUCCESS;SUCCESS;SUCCESS;FAILURE</Value>
    </Parameter>
    <Parameter>
      <Name>COMMENT</Name>
      <Value>One Dummy failure occured</Value>
    </Parameter>
    <Parameter>
      <Name>DURATION</Name>
      <Value>0</Value>
    </Parameter>
  </Parameters>
</TestCase>
