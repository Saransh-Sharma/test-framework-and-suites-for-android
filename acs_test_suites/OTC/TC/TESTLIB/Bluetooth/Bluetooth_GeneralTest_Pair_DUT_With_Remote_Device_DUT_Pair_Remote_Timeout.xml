<?xml version='1.0' encoding='ASCII'?>
<TestCase>
    <UseCase>Testlib</UseCase>
    <Description>Pair request initiated by DUT, DUT will click Pair, remote device will wait for Timeout</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>2</TcMaxAttempt>
    <Parameters>
        <Parameter>
            <Name>test</Name>
            <Value>scripts/wireless/bluetooth/tests/Bluetooth_GeneralTest_Pair_tests.py</Value>
        </Parameter>
        <Parameter>
            <Name>script-args</Name>
            <Value>initiator=dut action_dut=Pair action_dev=Timeout action_initiator_first=true</Value>
        </Parameter>
        <Parameter>
            <Name>timeout</Name>
            <Value>10000</Value>
        </Parameter>
    </Parameters>
</TestCase>
