<?xml version='1.0' encoding='ASCII'?>
<TestCase>
    <UseCase>Testlib</UseCase>
    <Description>Pair request initiated by DUT , devices will Pair and than UNPair</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>1</TcMaxAttempt>
    <Parameters>
        <Parameter>
            <Name>test</Name>
            <Value>scripts/wireless/bluetooth/tests/Bluetooth_GeneralTest_Unpair_with_a_device.py</Value>
        </Parameter>
        <Parameter>
            <Name>script-args</Name>
            <Value>initiator=dut action_dut=Pair action_dev=Pair action_initiator_first=true</Value>
        </Parameter>
        <Parameter>
            <Name>timeout</Name>
            <Value>10000</Value>
        </Parameter>
    </Parameters>
</TestCase>
